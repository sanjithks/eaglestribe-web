{
  "name": "eaglestribe-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run tailwind:build && next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind:dev": "tailwindcss -i ./styles/input.css -o ./styles/output.css --watch",
    "tailwind:build": "tailwindcss -i ./styles/input.css -o ./styles/output.css --minify"
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resend": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.0",
    "tailwindcss": "^4.0.0"
  }
}